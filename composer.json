{
    "name": "ibudasov/sleeper",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "SleeperBundle\\": "src/SleeperBundle"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=7.1",
        "beberlei/assert": "^2.7",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "jms/serializer-bundle": "^2.2",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^3.0.2",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/symfony": "^3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "sensio/generator-bundle": "^3.0",
        "phpunit/phpunit": "^6.4",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "test:unit": "./vendor/bin/phpunit tests/SleeperBundle",
        "test:contract": "./vendor/bin/phpunit tests/contract",
        "test:coverage": "vendor/bin/phpunit --coverage-html var/test-coverage;  echo \"\nReport is located: var/test-coverage/index.html\"",
        "test:watch": "fswatch -o ./tests -o ./src | xargs -n1 -I{} ./vendor/bin/phpunit",
        "run": "bin/console server:start",
        "stop": "bin/console server:stop",
        "symfony-scripts": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "setup-database": [
            "bin/console doctrine:schema:update --force",
            "bin/console doctrine:schema:update --env=test --force"
        ],
        "post-install-cmd": [
            "@symfony-scripts",
            "@setup-database"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
